{
  "name": "react-keeper",
  "version": "1.0.1",
  "description": "A Strong Routing Library of React",
  "homepage": "https://github.com/vifird/react-keeper",
  "bug": "https://github.com/vifird/react-keeper/issues",
  "email": "vifirde@gmail.com",
  "scripts": {
    "example": "cross-env NODE_ENV=development node example/serve false",
    "test": "cross-env NODE_ENV=test karma start",
    "test-umd": "node __test__/serve",
    "build": "npm run build-umd && npm run build-min",
    "build-umd": "cross-env NODE_ENV=production webpack --config webpack.build.js modules/index.js umd/react-keeper.js",
    "build-min": "cross-env NODE_ENV=production webpack --config webpack.build.js -p modules/index.js umd/react-keeper.min.js",
    "precompile": "rimraf npm/__build__/**.js && rimraf npm/__build__/**/**.js",
    "compile": "cross-env NODE_ENV=production babel modules --extensions .js --out-dir npm/__build__",
    "prepublish": "npm run compile",
    "publish": "npm publish npm/__build__/ --tag latest"
  },
  "dependencies": {
    "react-boxer": "",
    "vhistory": "^1.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.10",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "codecov": "^1.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.2.0",
    "eslint-config-rackt": "^1.1.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.8.0",
    "react-functional": "^2.0.0",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "react-router": "next",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2"
  }
}
